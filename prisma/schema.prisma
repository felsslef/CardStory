generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuario {
  Nome      String   @db.VarChar(30)
  Sobrenome String   @db.VarChar(30)
  Username  String   @id @db.VarChar(15)
  Sexo      SexoEnum?
  DataNasc  DateTime?
  Email     String   @unique @db.VarChar(35)
  Senha     String   @db.VarChar(15)

  @@map("usuario")
}

enum SexoEnum {
  M
  F
  N
}